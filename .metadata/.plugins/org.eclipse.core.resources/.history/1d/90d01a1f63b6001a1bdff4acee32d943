import java.util.LinkedList;

public class LinkedList_example {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
LinkedList<String> li= new LinkedList<String>();
li.add("MOnday");
li.add("Tuesday");
li.add("Wednesday");
li.set(0, "Sunday");

System.out.println(li);
	}

}
