	public WebElement 
	
	@FindBy(xpath = "")
	public WebElement 
	
	@FindBy(xpath = "")
	public WebElement 
	
	@FindBy(xpath = "")
	public WebElement 
	
	@FindBy(xpath = "")
	public WebElement 
	
	@FindBy(xpath = "")
	public WebElement 
	
	@FindBy(xpath = "")
	public WebElement 
	
	@FindBy(xpath = "")
	public WebElement 
	
	@FindBy(xpath = "")
	public WebElement ;
	
	public CouponsPage(WebDriver driver)
	{
		PageFactory.initElements(driver, this);
	}
	
	
	
	public void click_coupons_list_link()
	{
		coupons_list_link.click();
	}
}
