package Java_Interview_questions;

public class String_contains_digits_only {

	public static void main(String[] args) {
		String str="";
	
	if(str.matches("[0-9]+"))
	{
		System.out.println("String contains only digit");
	}
	else
	{
		System.out.println("string contains alphanumeric char");
	}
	
	}

}
